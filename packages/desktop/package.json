{
  "name": "@moicad/desktop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "dev:tauri": "cargo tauri dev",
    "build": "cd frontend && npm run build",
    "build:tauri": "cargo tauri build",
    "build:tauri:linux": "cargo tauri build --target x86_64-unknown-linux-gnu",
    "build:tauri:mac": "cargo tauri build --target aarch64-apple-darwin",
    "build:tauri:win": "cargo tauri build --target x86_64-pc-windows-msvc",
    "start": "cd frontend && npm run start",
    "test": "npm test",
    "type-check": "npm run type-check"
  },
  "dependencies": {
    "@moicad/sdk": "workspace:*",
    "next": "^16.1.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "three": "^0.182.0",
    "three-bvh-csg": "^0.0.17",
    "@monaco-editor/react": "^4.6.0",
    "monaco-editor": "^0.50.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.160.0",
    "typescript": "^5.0.0",
    "bun-types": "latest"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
