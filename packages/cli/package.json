{
  "name": "@moicad/cli",
  "version": "0.1.12",
  "type": "module",
  "description": "Modern JavaScript CAD Platform - CLI tool",
  "bin": {
    "moicad": "./dist/index.js"
  },
  "files": [
    "dist",
    "static",
    "scripts",
    "README.md"
  ],
  "scripts": {
    "build:gui-bundle": "vite build",
    "build:copy-wasm": "node scripts/copy-wasm.js",
    "build": "bun run build:gui-bundle && bun build src/index.ts --target=bun --outdir=dist && chmod +x dist/index.js && bun run build:copy-wasm",
    "dev": "bun src/index.ts",
    "clean": "rm -rf dist static/gui-bundle.js",
    "dev:server": "bun src/server.ts",
    "test": "bash scripts/test-cli.sh",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@moicad/sdk": "^0.1.14",
    "@moicad/gui": "^0.1.6",
    "manifold-3d": "^3.3.2",
    "open": "^10.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "bun-types": "latest",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.31.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "cad",
    "3d",
    "modeling",
    "cli",
    "openscad",
    "moicad"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moikas/moicad",
    "directory": "packages/cli"
  },
  "license": "MIT"
}
