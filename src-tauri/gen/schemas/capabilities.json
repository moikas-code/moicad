{
  "default": {
    "identifier": "default",
    "description": "Default capabilities for moicad",
    "local": true,
    "permissions": [
      "core:default",
      "shell:allow-open",
      "http:default",
      {
        "identifier": "http:allow-fetch",
        "allow": [{ "url": "http://localhost:42069/*" }]
      }
    ],
    "platforms": ["linux", "macOS", "windows"]
  }
}
