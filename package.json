{
  "name": "moicad",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --hot ./backend/index.ts",
    "dev:frontend": "cd frontend && npm run dev -- -p 3001",
    "dev:all": "concurrently \"bun run dev\" \"bun run dev:frontend\"",
    "build": "bun run build:wasm && bun run build:frontend",
    "build:wasm": "cd wasm && wasm-pack build --target web",
    "build:frontend": "cd frontend && npm run build",
    "start": "node frontend/.next/server/app.js",
    "test": "bun test",
    "tauri:dev": "cargo tauri dev",
    "tauri:build": "cargo tauri build",
    "tauri:build:linux": "cargo tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:mac": "cargo tauri build --target aarch64-apple-darwin",
    "tauri:build:win": "cargo tauri build --target x86_64-pc-windows-msvc"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "^14.0.0",
    "three": "^0.160.0",
    "monaco-editor": "^0.50.0",
    "@monaco-editor/react": "^4.6.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.160.0",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
